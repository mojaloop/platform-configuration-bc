{
  "name": "@mojaloop/platform-configuration-bc-global-configs-bootstrap-tool",
  "version": "0.1.4",
  "description": "Mojaloop platform configuration BC global configs definition and bootstrap tool",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/platform-configuration-bc/tree/main/modules/global-configs-bootstrap-tool",
  "bugs": {
    "url": "https://github.com/mojaloop/platform-configuration-bc/issues#readme"
  },
  "contributors": [
    "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
  ],
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "npm run clean:npm; npm run clean:dist",
    "clean:dist": "rm -Rf dist",
    "clean:npm": "rm -Rf node_modules",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "audit:resolve": "SHELL=sh resolve-audit",
    "audit:check": "SHELL=sh check-audit",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -e 2 -u"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@mojaloop/logging-bc-public-types-lib": "~0.1.19",
    "@mojaloop/platform-configuration-bc-public-types-lib": "*"
  }
}
