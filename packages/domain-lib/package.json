{
    "name": "@mojaloop/platform-configuration-bc-domain-lib",
    "version": "0.5.0",
    "description": "mojaloop platform domain lib",
    "license": "Apache-2.0",
    "homepage": "https://github.com/mojaloop/platform-configuration-bc/tree/main/modules/domain-lib",
    "private": true,
    "bugs": {
        "url": "https://github.com/mojaloop/platform-configuration-bc/issues#readme"
    },
    "contributors": [
        "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "clean": "npm run clean:npm; npm run clean:dist",
        "clean:dist": "rm -Rf dist",
        "clean:npm": "rm -Rf node_modules",
        "pretest": "npm run build",
        "standard": "npm run lint",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "test:unit": "jest --passWithNoTests --testMatch '**/test/unit/**/*.test.ts'",
        "audit:resolve": "SHELL=sh resolve-audit",
        "audit:check": "SHELL=sh check-audit",
        "dep:check": "ncu -e 2",
        "dep:update": "ncu -e 2 -u"
    },
    "dependencies": {
        "@mojaloop/platform-shared-lib-messaging-types-lib": "~0.5.6",
        "@mojaloop/platform-shared-lib-public-messages-lib": "~0.5.12",
        "@mojaloop/security-bc-public-types-lib": "~0.5.6",
        "@mojaloop/auditing-bc-client-lib": "~0.5.2",
        "@mojaloop/auditing-bc-public-types-lib": "~0.5.2",
        "@mojaloop/logging-bc-client-lib": "~0.5.6",
        "@mojaloop/platform-configuration-bc-public-types-lib": "*",
        "semver": "^7.6.0",
        "ajv": "^8.12.0"
    },
    "devDependencies": {
        "@types/semver": "^7.5.8"
    },
    "engines": {
        "node": ">=20.10.0"
    }
}
